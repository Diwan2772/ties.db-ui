    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "ethjs-provider-signer": "^0.1.4",
    "ethjs-signer": "^0.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "hash.js": "^1.1.3",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "metamask-logo": "^2.1.3",
    "node-eth-address": "^0.1.3",
    "node-loader": "^0.6.0",
    "normalizr": "^3.2.4",
    "npm": "^5.7.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-ace": "^6.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-css-transition": "^0.7.4",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-json-view": "^1.19.1",
    "react-portal": "^4.1.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-table": "^6.8.6",
    "recompose": "^0.27.1",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "scrypt": "^6.0.3",
    "string-to-color": "^2.0.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "tiesdb-client": "^0.1.2",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "0.6.2",
    "web3": "^1.0.0-beta.36",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "electron .",
    "electron-start": "node app/react.js",
    "react-start": "node scripts/start.js BROWSER=none",
    "dist": "yarn build && build",
    "pack": "build --dir"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-builder": "^20.28.4",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6"
  },
  "homepage": "./",
  "main": "app/start.js",
  "build": {
    "appId": "com.ties.schema-designer",
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "icon": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    }
  }
}
